<Window x:Class="pdf_manager.ConfirmationBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pdf_manager"
        mc:Ignorable="d"
        Title="ConfirmationBox" Height="450" Width="800">
    <Grid>
        <Label x:Name="information" Content="Label" HorizontalAlignment="Left" Height="87" Margin="134,71,0,0" VerticalAlignment="Top" Width="474"/>
        <Button x:Name="accept" Content="OK" HorizontalAlignment="Left" Height="77" Margin="134,189,0,0" VerticalAlignment="Top" Width="173" Click="accept_Click"/>
        <Button x:Name="cancel" Content="Cancel" HorizontalAlignment="Left" Height="77" Margin="400,189,0,0" VerticalAlignment="Top" Width="173" Click="cancel_Click"/>

    </Grid>
</Window>
