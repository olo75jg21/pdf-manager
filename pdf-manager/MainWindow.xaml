<Window x:Class="pdf_manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pdf_manager"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True" Margin="0,0,0,0">
        <ToolBar DockPanel.Dock="Top" Height="28" Header="">
            <Label>"Toolbar"</Label>
        </ToolBar>
        <ToolBar DockPanel.Dock="Bottom">
            <Label>"Footer"</Label>
        </ToolBar>
        <StackPanel DockPanel.Dock="Left" Background="Red">
            <Label>"StackPanel"</Label>
        </StackPanel>
        <WrapPanel Background="Yellow">
            <Label>"Workspace"</Label>
        </WrapPanel>
    </DockPanel>
</Window>
