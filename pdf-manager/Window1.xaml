<Window x:Class="pdf_manager.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pdf_manager"
        mc:Ignorable="d"
        Title="Window1" Height="380" Width="593">
    <Grid Margin="0,0,0,-6">
        <Label x:Name="password" Content="Stare Hasło:" HorizontalAlignment="Left" Height="30" Margin="183,124,0,0" VerticalAlignment="Top" Width="91"/>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Height="33" Margin="183,212,0,0" VerticalAlignment="Top" Width="91" Click="okButton_Click"/>
        <Button x:Name="cancelButton" Content="CANCEL" HorizontalAlignment="Left" Height="33" Margin="296,212,0,0" VerticalAlignment="Top" Width="118" Click="cancelButton_Click"/>
        <TextBox x:Name="oldPassword" HorizontalAlignment="Left" Height="30" Margin="274,124,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="121"/>
        <Label Content="Nowe Hasło:" HorizontalAlignment="Left" Height="28" Margin="183,161,0,0" VerticalAlignment="Top" Width="91"/>
        <TextBox x:Name="newPassword" HorizontalAlignment="Left" Height="30" Margin="276,160,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="121"/>

    </Grid>
</Window>
