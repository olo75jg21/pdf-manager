<Window x:Class="pdf_manager.CustomMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pdf_manager"
        mc:Ignorable="d"
        Title="CustomMessageBox" Height="220" Width="476">
    <Grid Margin="0,0,0,-6">
        <Label x:Name="information" Content="Label" HorizontalAlignment="Center" Height="76" VerticalAlignment="Top" Width="405" Margin="0,24,0,0"/>
        <Button x:Name="buttonOK" Content="OK" HorizontalAlignment="Center" Height="42" Margin="0,105,0,0" VerticalAlignment="Top" Width="246" Click="buttonOK_Click"/>

    </Grid>
</Window>
